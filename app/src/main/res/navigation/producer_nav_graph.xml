<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/producer_nav_graph"
    app:startDestination="@id/onboardingFragment">

    <fragment
        android:id="@+id/onboardingFragment"
        android:name="com.powerhungers.fomezero.feature.onboarding.presentation.OnboardingFragment"
        android:label="fragment_onboarding"
        tools:layout="@layout/fragment_onboarding" >
        <action
            android:id="@+id/navigateToHome"
            app:destination="@id/producerHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/producerHomeFragment"
        android:name="com.powerhungers.fomezero.feature.producer.home.presentation.ProducerHomeFragment"
        android:label="fragment_producer_home"
        tools:layout="@layout/fragment_producer_home" >
        <action
            android:id="@+id/navigate_to_basketFragment"
            app:destination="@id/basketFragment" />
    </fragment>
    <fragment
        android:id="@+id/basketFragment"
        android:name="com.powerhungers.fomezero.feature.producer.basket.presentation.BasketFragment"
        android:label="fragment_basket"
        tools:layout="@layout/fragment_basket" >
        <action
            android:id="@+id/navigate_to_producerHomeFragment"
            app:destination="@id/producerHomeFragment" />
    </fragment>
</navigation>